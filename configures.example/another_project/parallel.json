{
  "configures": [
    {
      "file_name": "configure-1.json",
      "alias": "$configure_first_configure",
      "tes" : "1"
    },
    {
      "file_name": "configure-2.json",
      "alias": "$configure_second_configure",
      "tes" : "1"
    }
  ],
  "next_failure" : {
    "status_code" : 500,
    "transform" : "ToJson",
    "adds" : {
      "body" : {
        "error_message" : "all logics is fail",
        "response_second_configure" : "$configure_second_configure_0--$response--$status_code",
        "error" : "$configure_second_configure_0-$response--$body"
      }
    }
  },
  "c_logics": [
    {
      "rule": {
        "==": [
          "$configure_first_configure_0--$response--$status_code",
          400
        ]
      },
      "data": null,
      "next_success" : "",
      "response": {
        "status_code" : 400,
        "transform" : "ToJson",
        "adds" : {
          "body" : {
            "message" : "configure_first configure status code is 400"
          }
        }
      }
    },
    {
      "rule": {
        "==": [
          "$configure_second_configure_0--$response--$status_code",
          200
        ]
      },
      "data": null,
      "next_success":"",
      "response": {
        "status_code" : 200,
        "transform": "ToJson",
        "adds": {
          "header": {
            "key" : "123",
            "userID" : "userid"
          },
          "body": {
            "user.anime": "$configure_second_configure_0--$request--$body[user][favorite_anime]",
            "user.cars": "$configure_second_configure_0--$request--$body[user][favorite_cars]",
            "user.favorite_cars": "$configure_second_configure_0--$request--$body[user][favorite_cars][0]",
            "tes" : "zzzz"
          }
        },
        "modifies": {
          "header" :{},
          "body": {
          }
        },
        "deletes": {
          "header" : [],
          "body": [
          ]
        }
      }
    }
  ]
}