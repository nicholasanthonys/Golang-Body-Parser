{
  "destinationUrl": "http://localhost:3001",
  "method": ["PUT"],
  "request": {
    "transform": "ToXml",
    "adds": {
      "header": {
        "tes": "ololol",
        "headeraddress": "$body[user][name][0]"
      },
      "body": {
        "user.address": "$header[Content-Type][0]",
        "user.age": "tes1",
        "user.query": "$query[tes][0]",
        "user.lol": "$body[user][name]"
      },
      "query": {
        "tesquery": "$header[Content-Type][0]"
      }
    },
    "deletes": {
      "header": [
      ],
      "body": [
      ],
      "query": [
      ]
    },
    "modifies": {
      "header": {
        "Tesheader" : "$header[Content-Type][0]"
      },
      "body": {


      },
      "query": {
      }
    }
  },
  "response": {
    "transform": "ToJson",
    "adds": {
      "header": {
        "tesheaderresponse": "$header[Content-Type]"
      },
      "body": {
        "user.name": "$header[Content-Type][0]",
        "user.response": "$header[tesheaderresponse]"
      },
      "query": {
      }
    },
    "deletes": {
      "body": [
      ]
    },
    "modifies": {
      "body": {
      }
    }
  }
}
